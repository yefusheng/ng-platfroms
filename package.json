{
  "name": "angular-platforms",
  "version": "0.1.5",
  "description": "Angular -Multi platform single interface",
  "main": "dist/index.js",
  "typings": "index.d.ts",
  "author": "yefusheng",
  "license": "MIT",
  "module": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "clean-generated": "rm -rf src/*.ngfactory.ts && rm -rf src/*.ngsummary.json",
    "bundling": "rollup -c",
    "minify": "uglifyjs dist/bundles/ng-platforms.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng-platforms.umd.min.js",
    "copy": "copyfiles README.md dist",
    "build": "npm run clean && ngc && npm run bundling && npm run minify && npm run copy"
  },
  "dependencies": {
    "@angular/common": "^4.3.5",
    "rxjs": "5.4.0"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular4"
  ],
  "devDependencies": {
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@ionic/app-scripts": "^1.3.11",
    "@ionic-native/app-version": "^3.4.2",
    "@ionic-native/call-number": "^3.5.0",
    "@ionic-native/camera": "^3.4.2",
    "@ionic-native/core": "^3.6.1",
    "@ionic-native/file": "^3.4.2",
    "@ionic-native/file-opener": "^3.4.2",
    "@ionic-native/image-picker": "^3.4.2",
    "@ionic-native/in-app-browser": "^3.4.2",
    "@ionic-native/network": "^3.4.2",
    "@ionic-native/splash-screen": "3.4.2",
    "@ionic-native/status-bar": "3.4.2",
    "@ionic-native/toast": "^3.4.2",
    "@ionic-native/transfer": "^3.4.2",
    "@ionic/storage": "2.0.1",
    "ionic-angular": "3.5.2",
    "ionic-native": "^2.9.0",
    "@types/gulp": "^4.0.3",
    "copyfiles": "^1.2.0",
    "core-js": "2.4.1",
    "gulp": "3.9.1",
    "typescript": "~2.3.3",
    "gulp-tsc": "^1.3.2",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "tslib": "^1.7.1",
    "uglify-js": "^3.0.27"
  },
